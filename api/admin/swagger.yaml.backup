                    - totalUsers
                    - newUsers
                    - activeUsers
                    - userGrowth
                    - userDistribution
                    - topUsers
                    type: object
                type: object
              message:
                description: 响应消息
                type: string
            type: object
      schemes:
      - https
  /api/admin/data/country/:
    get:
      operationId: listCountries
      parameters:
      - allowEmptyValue: true
        default: 1
        description: 页码
        in: query
        name: page
        type: integer
      - allowEmptyValue: true
        default: 10
        description: 每页数量
