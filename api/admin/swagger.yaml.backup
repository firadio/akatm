basePath: /
consumes:
- application/json
info:
  version: "1.0"
paths:
  /api/admin/dashboard/:
    get:
      operationId: getDashboardData
      parameters:
      - allowEmptyValue: true
        description: 需要返回的模块，逗号分隔：overview,user,fund,transaction,audit,customer,account,kyc,realtime,alerts。不传则返回核心模块
        in: query
        name: modules
        type: string
      - allowEmptyValue: true
        default: 7d
        description: 时间范围：1d、7d、30d、90d
        in: query
        name: timeRange
        type: string
      produces:
      - application/json
      responses:
        "200":
          description: ""
          schema:
            properties:
              code:
                description: 响应码
                type: integer
              data:
                description: 数据
                properties:
                  accountStats:
                    description: 账户统计
                    properties:
                      accountGrowth:
                        description: 账户增长趋势
                        items:
                          properties:
                            date:
                              description: 日期（毫秒级时间戳）
                              type: integer
                            value:
                              description: 数值
                              type: integer
                          required:
                          - date
                          - value
                          type: object
                        type: array
                      accountStatusDistribution:
                        description: 账户状态分布
                        items:
                          properties:
                            name:
                              description: 名称
                              type: string
                            percentage:
                              description: 百分比
                              type: number
                            value:
                              description: 数值
                              type: integer
                          required:
                          - name
                          - value
                          - percentage
                          type: object
                        type: array
                      accountTypeDistribution:
                        description: 账户类型分布
                        items:
                          properties:
                            name:
                              description: 名称
                              type: string
                            percentage:
                              description: 百分比
                              type: number
                            value:
                              description: 数值
                              type: integer
                          required:
                          - name
                          - value
                          - percentage
                          type: object
                        type: array
                      activeAccounts:
                        description: 活跃账户数
                        type: integer
                      frozenAccounts:
                        description: 冻结账户数
                        type: integer
                      totalAccounts:
                        description: 总账户数
                        type: integer
                    required:
                    - totalAccounts
                    - activeAccounts
                    - frozenAccounts
                    - accountGrowth
                    - accountTypeDistribution
                    - accountStatusDistribution
                    type: object
                  alerts:
                    description: 告警信息
                    properties:
                      list:
                        description: 告警列表
                        items:
                          properties:
                            createdAt:
                              description: 创建时间
                              type: string
                            currentValue:
                              description: 当前值
                              type: string
                            description:
                              description: 告警描述
                              type: string
                            id:
                              description: 告警ID
                              type: integer
                            level:
                              description: 告警级别：info、warning、error、critical
                              type: string
                            metric:
                              description: 指标
                              type: string
                            resolvedAt:
                              description: 解决时间
                              type: string
                            resolvedBy:
                              description: 解决人
                              type: string
                            source:
                              description: 告警源
                              type: string
                            status:
                              description: 状态：active、resolved
                              type: string
                            threshold:
                              description: 阈值
                              type: string
                            title:
                              description: 告警标题
                              type: string
                          required:
                          - id
                          - title
                          - description
                          - level
                          - status
                          - source
                          - metric
                          - threshold
                          - currentValue
                          - createdAt
                          type: object
                        type: array
                      summary:
                        description: 告警摘要
                        properties:
                          activeAlerts:
                            description: 活跃告警数
                            type: integer
                          criticalAlerts:
                            description: 严重告警数
                            type: integer
                          resolvedToday:
                            description: 今日解决数
                            type: integer
                          totalAlerts:
                            description: 总告警数
                            type: integer
                          warningAlerts:
                            description: 警告告警数
                            type: integer
                        required:
                        - totalAlerts
                        - activeAlerts
                        - criticalAlerts
                        - warningAlerts
                        - resolvedToday
                        type: object
                      total:
                        description: 总记录数
                        type: integer
                    required:
                    - total
                    - list
                    - summary
                    type: object
                  auditStats:
                    description: 审核统计
                    properties:
                      approvalRate:
                        description: 通过率
                        type: number
                      approvedAudits:
                        description: 已通过审核数
                        type: integer
                      auditTrend:
                        description: 审核趋势
                        items:
                          properties:
                            approved:
                              description: 已通过
                              type: integer
                            date:
                              description: 日期（毫秒级时间戳）
                              type: integer
                            pending:
                              description: 待审核
                              type: integer
                            rejected:
                              description: 已拒绝
                              type: integer
                          required:
                          - date
                          - pending
                          - approved
                          - rejected
                          type: object
                        type: array
                      auditTypes:
                        description: 审核类型统计
                        items:
                          properties:
                            approved:
                              description: 已通过
                              type: integer
                            count:
                              description: 审核数
                              type: integer
                            pending:
                              description: 待审核
                              type: integer
                            rejected:
                              description: 已拒绝
                              type: integer
                            type:
                              description: 审核类型
                              type: string
                          required:
                          - type
                          - count
                          - pending
                          - approved
                          - rejected
                          type: object
                        type: array
                      auditorStats:
                        description: 审核员统计
                        items:
                          properties:
                            approvedAudits:
                              description: 已通过审核数
                              type: integer
                            auditorId:
                              description: 审核员ID
                              type: integer
                            auditorName:
                              description: 审核员姓名
                              type: string
                            averageProcessTime:
                              description: 平均处理时间
                              type: integer
                            rejectedAudits:
                              description: 已拒绝审核数
                              type: integer
                            totalAudits:
                              description: 总审核数
                              type: integer
                          required:
                          - auditorId
                          - auditorName
                          - totalAudits
                          - approvedAudits
                          - rejectedAudits
                          - averageProcessTime
                          type: object
                        type: array
                      averageProcessTime:
                        description: 平均处理时间（分钟）
                        type: integer
                      pendingAudits:
                        description: 待审核数
                        type: integer
                      rejectedAudits:
                        description: 已拒绝审核数
                        type: integer
                      totalAudits:
                        description: 总审核数
                        type: integer
                    required:
                    - totalAudits
                    - pendingAudits
                    - approvedAudits
                    - rejectedAudits
                    - approvalRate
                    - averageProcessTime
                    - auditTrend
                    - auditTypes
                    - auditorStats
                    type: object
                  customerStats:
                    description: 客户统计
                    properties:
                      countryDistribution:
                        description: 国家分布
                        items:
                          properties:
                            countryCode:
                              description: 国家代码
                              type: string
                            countryName:
                              description: 国家名称
                              type: string
                            customerCount:
                              description: 客户数量
                              type: integer
                            percentage:
                              description: 百分比
                              type: number
                          required:
                          - countryCode
                          - countryName
                          - customerCount
                          - percentage
                          type: object
                        type: array
                      customerGrowth:
                        description: 客户增长趋势
                        items:
                          properties:
                            date:
                              description: 日期（毫秒级时间戳）
                              type: integer
                            value:
                              description: 数值
                              type: integer
                          required:
                          - date
                          - value
                          type: object
                        type: array
                      customerTypeDistribution:
                        description: 客户类型分布
                        items:
                          properties:
                            name:
                              description: 名称
                              type: string
                            percentage:
                              description: 百分比
                              type: number
                            value:
                              description: 数值
                              type: integer
                          required:
                          - name
                          - value
                          - percentage
                          type: object
                        type: array
                      kycStatusDistribution:
                        description: KYC状态分布
                        items:
                          properties:
                            name:
                              description: 名称
                              type: string
                            percentage:
                              description: 百分比
                              type: number
                            value:
                              description: 数值
                              type: integer
                          required:
                          - name
                          - value
                          - percentage
                          type: object
                        type: array
                      newCustomers:
                        description: 新增客户数
                        type: integer
                      pendingKycCustomers:
                        description: 待KYC客户数
                        type: integer
                      topCustomers:
                        description: 活跃客户排行
                        items:
                          properties:
                            accountCount:
                              description: 账户数量
                              type: integer
                            customerId:
                              description: 客户ID
                              type: integer
                            customerName:
                              description: 客户姓名
                              type: string
                            customerType:
                              description: 客户类型
                              type: string
                            lastTransactionAt:
                              description: 最后交易时间
                              type: string
                            totalBalance:
                              description: 总余额
                              type: string
                          required:
                          - customerId
                          - customerName
                          - customerType
                          - accountCount
                          - totalBalance
                          - lastTransactionAt
                          type: object
                        type: array
                      totalCustomers:
                        description: 总客户数
                        type: integer
                      verifiedCustomers:
                        description: 已认证客户数
                        type: integer
                    required:
                    - totalCustomers
                    - newCustomers
                    - verifiedCustomers
                    - pendingKycCustomers
                    - customerGrowth
                    - customerTypeDistribution
                    - kycStatusDistribution
                    - countryDistribution
                    - topCustomers
                    type: object
                  fundStats:
                    description: 资金统计
                    properties:
                      currencyStats:
                        description: 币种统计
                        items:
                          properties:
                            currency:
                              description: 币种
                              type: string
                            depositAmount:
                              description: 存款金额
                              type: string
                            feeAmount:
                              description: 手续费金额
                              type: string
                            transactionCount:
                              description: 交易笔数
                              type: integer
                            withdrawalAmount:
                              description: 提现金额
                              type: string
                          required:
                          - currency
                          - depositAmount
                          - withdrawalAmount
                          - feeAmount
                          - transactionCount
                          type: object
                        type: array
                      dailyFunds:
                        description: 每日资金
                        items:
                          properties:
                            date:
                              description: 日期（毫秒级时间戳）
                              type: integer
                            deposit:
                              description: 存款
                              type: string
                            fee:
                              description: 手续费
                              type: string
                            net:
                              description: 净额
                              type: string
                            withdrawal:
                              description: 提现
                              type: string
                          required:
                          - date
                          - deposit
                          - withdrawal
                          - fee
                          - net
                          type: object
                        type: array
                      fundFlow:
                        description: 资金流向
                        items:
                          properties:
                            amount:
                              description: 金额
                              type: string
                            percentage:
                              description: 百分比
                              type: number
                            type:
                              description: 类型：deposit、withdrawal、fee
                              type: string
                          required:
                          - type
                          - amount
                          - percentage
                          type: object
                        type: array
                      netAmount:
                        description: 净金额
                        type: string
                      totalDeposit:
                        description: 总存款
                        type: string
                      totalFee:
                        description: 总手续费
                        type: string
                      totalWithdrawal:
                        description: 总提现
                        type: string
                    required:
                    - totalDeposit
                    - totalWithdrawal
                    - totalFee
                    - netAmount
                    - fundFlow
                    - currencyStats
                    - dailyFunds
                    type: object
                  kycStats:
                    description: KYC统计
                    properties:
                      approvedKycApplications:
                        description: 已通过KYC申请数
                        type: integer
                      averageKycProcessTime:
                        description: 平均KYC处理时间（小时）
                        type: integer
                      kycApprovalRate:
                        description: KYC通过率
                        type: number
                      kycCountryDistribution:
                        description: KYC国家分布
                        items:
                          properties:
                            countryCode:
                              description: 国家代码
                              type: string
                            countryName:
                              description: 国家名称
                              type: string
                            customerCount:
                              description: 客户数量
                              type: integer
                            percentage:
                              description: 百分比
                              type: number
                          required:
                          - countryCode
                          - countryName
                          - customerCount
                          - percentage
                          type: object
                        type: array
                      kycTrend:
                        description: KYC趋势
                        items:
                          properties:
                            approved:
                              description: 已通过
                              type: integer
                            date:
                              description: 日期（毫秒级时间戳）
                              type: integer
                            pending:
                              description: 待审核
                              type: integer
                            rejected:
                              description: 已拒绝
                              type: integer
                          required:
                          - date
                          - pending
                          - approved
                          - rejected
                          type: object
                        type: array
                      kycTypeDistribution:
                        description: KYC类型分布
                        items:
                          properties:
                            name:
                              description: 名称
                              type: string
                            percentage:
                              description: 百分比
                              type: number
                            value:
                              description: 数值
                              type: integer
                          required:
                          - name
                          - value
                          - percentage
                          type: object
                        type: array
                      pendingKycApplications:
                        description: 待审核KYC申请数
                        type: integer
                      rejectedKycApplications:
                        description: 已拒绝KYC申请数
                        type: integer
                      totalKycApplications:
                        description: 总KYC申请数
                        type: integer
                    required:
                    - totalKycApplications
                    - pendingKycApplications
                    - approvedKycApplications
                    - rejectedKycApplications
                    - kycApprovalRate
                    - averageKycProcessTime
                    - kycTrend
                    - kycTypeDistribution
                    - kycCountryDistribution
                    type: object
                  overview:
                    description: 概览数据
                    properties:
                      quickStats:
                        description: 快速统计
                        properties:
                          activeUsers:
                            description: 活跃用户数
                            type: integer
                          pendingAudits:
                            description: 待审核数
                            type: integer
                          systemUptime:
                            description: 系统运行时间
                            type: integer
                          totalAmount:
                            description: 总金额
                            type: string
                          totalTransactions:
                            description: 总交易数
                            type: integer
                          totalUsers:
                            description: 总用户数
                            type: integer
                        required:
                        - totalUsers
                        - activeUsers
                        - totalTransactions
                        - totalAmount
                        - pendingAudits
                        - systemUptime
                        type: object
                      recentActivities:
                        description: 最近活动
                        items:
                          properties:
                            description:
                              description: 描述
                              type: string
                            id:
                              description: 活动ID
                              type: integer
                            status:
                              description: 状态
                              type: string
                            time:
                              description: 时间（毫秒级时间戳）
                              type: integer
                            type:
                              description: 活动类型
                              type: string
                            userName:
                              description: 用户名
                              type: string
                            userType:
                              description: 用户类型
                              type: string
                          required:
                          - id
                          - type
                          - description
                          - userName
                          - userType
                          - time
                          - status
                          type: object
                        type: array
                      summaryCards:
                        description: 概览卡片
                        items:
                          properties:
                            change:
                              description: 变化
                              type: string
                            changeType:
                              description: 变化类型：increase、decrease、stable
                              type: string
                            color:
                              description: 颜色
                              type: string
                            icon:
                              description: 图标
                              type: string
                            title:
                              description: 标题
                              type: string
                            unit:
                              description: 单位
                              type: string
                            value:
                              description: 数值
                              type: string
                          required:
                          - title
                          - value
                          - unit
                          - change
                          - changeType
                          - icon
                          - color
                          type: object
                        type: array
                      systemHealth:
                        description: 系统健康状态
                        properties:
                          cpuUsage:
                            description: CPU使用率
                            type: number
                          databaseStatus:
                            description: 数据库状态
                            type: string
                          diskUsage:
                            description: 磁盘使用率
                            type: number
                          lastCheck:
                            description: 最后检查时间
                            type: string
                          memoryUsage:
                            description: 内存使用率
                            type: number
                          redisStatus:
                            description: Redis状态
                            type: string
                          status:
                            description: 状态：healthy、warning、critical
                            type: string
                        required:
                        - status
                        - cpuUsage
                        - memoryUsage
                        - diskUsage
                        - databaseStatus
                        - redisStatus
                        - lastCheck
                        type: object
                    required:
                    - summaryCards
                    - quickStats
                    - recentActivities
                    - systemHealth
                    type: object
                  realtimeMonitor:
                    description: 实时监控
                    properties:
                      activeConnections:
                        description: 活跃连接数
                        type: integer
                      errorRate:
                        description: 错误率
                        type: number
                      onlineUsers:
                        description: 在线用户数
                        type: integer
                      recentRequests:
                        description: 最近请求
                        items:
                          properties:
                            method:
                              description: 请求方法
                              type: string
                            path:
                              description: 请求路径
                              type: string
                            responseTime:
                              description: 响应时间
                              type: integer
                            statusCode:
                              description: 状态码
                              type: integer
                            timestamp:
                              description: 时间戳
                              type: integer
                          required:
                          - method
                          - path
                          - statusCode
                          - responseTime
                          - timestamp
                          type: object
                        type: array
                      requestRate:
                        description: 请求速率（每分钟）
                        type: integer
                      responseTime:
                        description: 平均响应时间（毫秒）
                        type: number
                      systemLoad:
                        description: 系统负载
                        properties:
                          cpuUsage:
                            description: CPU使用率
                            type: number
                          diskUsage:
                            description: 磁盘使用率
                            type: number
                          memoryUsage:
                            description: 内存使用率
                            type: number
                          networkIn:
                            description: 网络入流量
                            type: number
                          networkOut:
                            description: 网络出流量
                            type: number
                        required:
                        - cpuUsage
                        - memoryUsage
                        - diskUsage
                        - networkIn
                        - networkOut
                        type: object
                    required:
                    - onlineUsers
                    - activeConnections
                    - requestRate
                    - responseTime
                    - errorRate
                    - systemLoad
                    - recentRequests
                    type: object
                  transactionStats:
                    description: 交易统计
                    properties:
                      failedTransactions:
                        description: 失败交易数
                        type: integer
                      hourlyDistribution:
                        description: 小时分布
                        items:
                          properties:
                            count:
                              description: 交易数
                              type: integer
                            hour:
                              description: 小时
                              type: integer
                          required:
                          - hour
                          - count
                          type: object
                        type: array
                      successRate:
                        description: 成功率
                        type: number
                      successTransactions:
                        description: 成功交易数
                        type: integer
                      totalTransactions:
                        description: 总交易数
                        type: integer
                      transactionTrend:
                        description: 交易趋势
                        items:
                          properties:
                            amount:
                              description: 交易金额
                              type: string
                            count:
                              description: 交易数
                              type: integer
                            date:
                              description: 日期（毫秒级时间戳）
                              type: integer
                          required:
                          - date
                          - count
                          - amount
                          type: object
                        type: array
                      transactionTypes:
                        description: 交易类型统计
                        items:
                          properties:
                            amount:
                              description: 交易金额
                              type: string
                            count:
                              description: 交易数
                              type: integer
                            percentage:
                              description: 百分比
                              type: number
                            type:
                              description: 交易类型
                              type: string
                          required:
                          - type
                          - count
                          - amount
                          - percentage
                          type: object
                        type: array
                    required:
                    - totalTransactions
                    - successTransactions
                    - failedTransactions
                    - successRate
                    - transactionTrend
                    - transactionTypes
                    - hourlyDistribution
                    type: object
                  userStats:
                    description: 用户统计
                    properties:
                      activeUsers:
                        description: 活跃用户数
                        type: integer
                      newUsers:
                        description: 新增用户数
                        type: integer
                      topUsers:
                        description: 活跃用户排行
                        items:
                          properties:
                            activityCount:
                              description: 活动次数
                              type: integer
                            customerId:
                              description: 客户ID
                              type: integer
                            lastActive:
                              description: 最后活跃时间
                              type: string
                            userName:
                              description: 用户名
                              type: string
                            userType:
                              description: 用户类型
                              type: string
                          required:
                          - customerId
                          - userName
                          - userType
                          - activityCount
                          - lastActive
                          type: object
                        type: array
                      totalUsers:
                        description: 总用户数
                        type: integer
                      userDistribution:
                        description: 用户分布
                        items:
                          properties:
                            name:
                              description: 名称
                              type: string
                            percentage:
                              description: 百分比
                              type: number
                            value:
                              description: 数值
                              type: integer
                          required:
                          - name
                          - value
                          - percentage
                          type: object
                        type: array
                      userGrowth:
                        description: 用户增长趋势
                        items:
                          properties:
                            date:
                              description: 日期（毫秒级时间戳）
                              type: integer
                            value:
                              description: 数值
                              type: integer
                          required:
                          - date
                          - value
                          type: object
                        type: array
